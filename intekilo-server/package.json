{
  "name": "server",
  "version": "1.0.0",
  "description": "production ready server",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "server:dev": "nodemon server.js",
    "server:prod": "cross-env NODE_ENV=production node server.js",
    "build:frontend": "cd ../intekilo-frontend && npm run build",
    "build:all": "npm run build:frontend && npm run start",
    "dev:full": "concurrently \"npm run dev\" \"cd ../intekilo-frontend && npm run dev\""
  },
  "author": "Me",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cryptr": "^6.0.3",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "mongodb": "^6.6.2",
    "mongoose": "^8.0.3",
    "redux-thunk": "^3.1.0",
    "socket.io": "^4.2.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cross-env": "^10.1.0",
    "nodemon": "^2.0.20"
  }
}
